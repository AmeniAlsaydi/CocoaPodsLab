# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CocoaPodsLab' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CocoaPodsLab

  # Alamofire networking library
  pod 'Alamofire' # not specifying the version number will install the most recent version of the library
  
  # SnapKit is a Autolayout third party library
  pod 'SnapKit'
  
  # Kingfisher is a photo processing library
  pod 'Kingfisher'

end
